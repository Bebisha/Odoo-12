<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Inherit stock.quant Form View to Modify it -->
    <record id="view_stock_quant_form_with_margin" model="ir.ui.view">
        <field name="name">view.stock.quant.form.with.margin</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='owner_id']" position="after">
                <field name="is_consignee_location"/>
            </xpath>
            <xpath expr="//field[@name='in_date']" position="before">
                <field name="product_category_id"/>
            </xpath>

        </field>
    </record>

     <record id="view_stock_view_stock_quant_tree" model="ir.ui.view">
        <field name="name">view.stock.quant.form.with.margin</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='reserved_quantity']" position="after">
                <field name="product_category_id"/>
            </xpath>
        </field>
    </record>

</odoo>